<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/darker_gray" />  <!-- Unfilled star color -->
        </shape>
    </item>
    <item android:right="0dp" android:left="0dp">
        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/holo_orange_dark" />  <!-- Filled star color -->
        </shape>
    </item>
</layer-list>
